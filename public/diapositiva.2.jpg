					<script>
function initJustifiedSwipers() {
    let swipers = document.querySelectorAll('.bobber_single--details-carousel .swiper, .bobber_single--details-carousel .swiper-container');
    swipers.forEach(swiperEl => {
        let swiperOptions = {
            slidesPerView: 3,
            spaceBetween: 80,  // Default space between slides
            centeredSlides: true,
            autoplay: false,
            shortSwipes: true,
            loop: true,
            threshold: 6,
            centeredSlidesBounds: true,
            slideToClickedSlide: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
  navigation: {
    nextEl: '.elementor-swiper-button-next',
    prevEl: '.elementor-swiper-button-prev',
  },
            breakpoints: {
                0: {
                    spaceBetween: 10,
                    slidesPerView: 1,
                },
                640: {
                    spaceBetween: 10,
                    slidesPerView: 1,
                },
                1024: {
                    spaceBetween: 80, 
                    slidesPerView: 3,
                }
            },
            on: {
                slideChange: function() {},
                init: function() {
                    this.slides.forEach(slide => {
                        slide.removeAttribute('inert');
                        slide.removeAttribute('aria-hidden');
                    });
                }
            }
        };

        if (swiperEl.swiper) {
            swiperEl.swiper.destroy();
        }

        new Swiper(swiperEl, swiperOptions);
    });
}

window.addEventListener('load', function() {
    if (document.querySelector('.elementor-editor-active')) {
        return;
    }

    let initAttempts = 0;
    let intervalId = setInterval(() => {
        try {
            initJustifiedSwipers();
            initAttempts++;
            let swipers = document.querySelectorAll('.bobber_single--details-carousel .swiper, .bobber_single--details-carousel .swiper-container');
            if (swipers[0].swiper.passedParams.slidesPerView === 'auto' || initAttempts > 30) {
                clearInterval(intervalId);
                if (initAttempts > 30) {
                    console.log('Swiper not found');
                }
            }
        } catch (error) {
            console.log(error);
        }
    }, 100);
});
</script>
